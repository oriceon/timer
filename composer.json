{
    "name": "oriceon/timer",
    "description": "",
    "keywords": [
        "oriceon",
        "timer"
    ],
    "homepage": "https://github.com/oriceon/timer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Valentin Iva»ôcu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Oriceon\\Timer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oriceon\\Timer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oriceon\\Timer\\TimerServiceProvider"
            ],
            "aliases": {
                "Timer": "Oriceon\\Timer\\TimerFacade"
            }
        }
    }
}
